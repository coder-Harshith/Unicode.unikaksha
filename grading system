a=int(input())
if(a==100):
    print('the grade is S')
elif(a<=99 and a>=90):
    print('the grade is A')
elif(a<=89 and a>=80):
    print('the grade is B')
elif(a<=79 and a>=70):
    print('the grade is C')
elif(a<=69 and a>=60):
    print('the grade is D')
elif(a<=59 and a>=50):
    print('the grade is E')
elif(a<50):
    print('the grade is F')
else:
    print('Invalid Input')
